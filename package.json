{
  "name": "orgmode-mcp",
  "version": "1.0.0",
  "description": "An MCP (Model Context Protocol) server for working with Org Mode files and workflows",
  "main": "dist/index.js",
  "bin": {
    "orgmode-mcp": "dist/index.js"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "clean": "rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate-config": "./scripts/generate-mcp-config.sh",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": ["mcp", "model-context-protocol", "claude", "ai", "server", "org-mode", "orgmode", "emacs"],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/node": "^24.9.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.1",
    "zod": "^3.25.76"
  }
}
